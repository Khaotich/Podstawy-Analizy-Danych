library(readxl)
sklepy=read_excel("sklepy.xls")
sklepy
sklep1=sklepy$Sklep1
sklep2=sklepy$Sklep2
# Sprawdzamy, czy dane zostaly pobrane z populacji, w których badana cecha ma rozk³ad normalny. 
shapiro.test(sklep1)
shapiro.test(sklep2)
# Dla testu Shapiro-Wilka H0 mówi, ¿e dane pochodz¹ z populacji o rozk³adzie normalym.
# Sprawdzamy, czy wariancje w populacjach, z których zosta³y pobrane dane s¹ takie same. 
# Test Fishera na równoœæ wariancji: 
var.test(sklep1, sklep2)
t.test(sklep1, sklep2)
t.test(sklep1, sklep2, var.equal=T)
a1=rnorm(50, 35, 3)
a2=rnorm(50, 32, 3)
a1
a2
shapiro.test(a1)
shapiro.test(a2)
var.test(a1, a2)
t.test(a1, a2, var.equal=T)
